{
  "canisters": {
    "hello": {
      "type": "custom",
      "wasm": "target/wasm32-unknown-unknown/release/hello-opt.wasm",
      "candid": "backend/hello/hello.did",
      "package": "hello",
      "build": "sh build.sh hello",
      "declarations": {
        "output": "src/declarations/hello",
        "node_compatibility": true
      }
    },
    "hello_assets": {
      "dependencies": [
        "hello"
      ],
      "frontend": {
        "entrypoint": "out/index.html"
      },
      "source": [
        "out"
      ],
      "type": "assets"
    }
  },
  "output_env_file": ".env",
  "version": 1
}